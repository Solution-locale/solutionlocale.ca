{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Solution-locale/commons"
        }
    ],
    "require": {
        "php": "^7.3",
        "alexpechkarev/google-maps": "8.0",
        "algolia/algoliasearch-client-php": "^2.6",
        "algolia/scout-extended": "^1.10",
        "axlon/laravel-postal-code-validation": "^3.1",
        "consoletvs/charts": "^6.5",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "geocodio/geocodio-library-php": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laracademy/interactive-make": "^1.4",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maatwebsite/excel": "^3.1",
        "propaganistas/laravel-phone": "^4.2.5",
        "solutionlocale/commons": "^2.4",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-backup": "^6.8",
        "spatie/laravel-honeypot": "^2.1",
        "spatie/laravel-permission": "^3.17",
        "thedoctor0/laravel-mailjet-driver": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
